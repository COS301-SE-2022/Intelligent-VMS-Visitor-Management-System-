{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@vms/my-library/(.*)": "<rootDir>/../libs/my-library/src/$1",
    "@vms/my-library": "<rootDir>/../libs/my-library/src",
    "@vms/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@vms/database": "<rootDir>/../libs/database/src",
    "@vms/visitor/(.*)": "<rootDir>/../libs/visitor/src/$1",
    "@vms/visitor": "<rootDir>/../libs/visitor/src",
    "@user/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "@user/user": "<rootDir>/../libs/user/src",
    "@vms/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@vms/auth": "<rootDir>/../libs/auth/src",
    "@vms/visitor-invite/(.*)": "<rootDir>/../libs/visitor-invite/src/$1",
    "@vms/visitor-invite": "<rootDir>/../libs/visitor-invite/src",
    "@vms/parking/(.*)": "<rootDir>/../libs/parking/src/$1",
    "@vms/parking": "<rootDir>/../libs/parking/src"
  }
}